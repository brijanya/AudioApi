MODEL FILE
class Type(models.Model):
    filetypes = [('song','Song'), ('podcast','Podcast'), ('audiobook','AudioBook')]
    id = models.PositiveIntegerField(primary_key=True)
    type = models.CharField(max_length = 100, choices = filetypes)
    name = models.CharField(max_length=100)
    duration = models.PositiveIntegerField()
    uploadTime = models.DateTimeField(auto_now=True)
    host = models.CharField(max_length=100, null=True)
    participants = models.ManyToManyField('PodcastParticipant')
    author = models.CharField(max_length=100, null=True)
    narrator = models.CharField(max_length=100, null=True)

VIEW FILE
class TypeViewSet(viewsets.ModelViewSet):
    queryset = Type.objects.all()
    serializer_class = TypeSerializer

SERIALIZERS
class TypeSerializer(serializers.HyperlinkedModelSerializer):
    class Meta:
        model = Type
        fields = ['id', 'type', 'name', 'duration', 'uploadTime', 'host', 'participants', 'author', 'narrator']
